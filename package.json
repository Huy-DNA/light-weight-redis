{
  "name": "light-weight-redis",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "jest --detectOpenHandles",
    "start": "node ./dist/bin/www.js",
    "build": "npx tsc && shx cp -r ./src/public ./src/views ./dist",
    "dev": "shx cp -r ./src/public ./src/views ./dist && concurrently \"npx tsc --watch\" \"nodemon --ignore log.json -q dist/index.js\"",
    "lint:js": "eslint --ext \".ts\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/debug": "^4.1.8",
    "@types/jest": "^29.5.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "ts-jest": "^29.1.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "@typescript-eslint/parser": "^5.59.9",
    "concurrently": "^8.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
